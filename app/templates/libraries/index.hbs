<div class="row">
  {{#each model as |library|}}
    <div class="col-md-4">
      {{#library-item item=library}}
        {{#link-to 'libraries.edit' library.id class='btn btn-success btn-xs'}}
<span class="glyphicon glyphicon-pencil" aria-hidden="true"></span>
        {{/link-to}}
        <button class="btn btn-danger btn-xs" {{action 'deleteLibrary' library}}>
<span class="glyphicon glyphicon-erase" aria-hidden="true"></span>
        </button>
      {{/library-item}}
    </div>
  {{/each}}
</div>