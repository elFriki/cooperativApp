<div class="row">
  {{#each model as |owner|}}
    <div class="col-md-4">
      {{#owner-item item=owner}}
          {{#link-to 'owners.edit' owner.id class='btn btn-success btn-xs'}}<span class="glyphicon glyphicon-pencil" aria-hidden="true"></span>{{/link-to}}
          <button class="btn btn-danger btn-xs" {{action 'deleteOwner' owner}}><span class="glyphicon glyphicon-erase" aria-hidden="true"></span></button>
      {{/owner-item}}
    </div>
  {{/each}}
</div>
